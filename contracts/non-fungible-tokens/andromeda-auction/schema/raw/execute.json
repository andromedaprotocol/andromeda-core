{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "andr_receive"
      ],
      "properties": {
        "andr_receive": {
          "$ref": "#/definitions/AndromedaMsg"
        }
