{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "description": "Instantiate Message for Swapper contract.",
  "type": "object",
  "required": [
    "primitive_contract",
    "swapper_impl"
  ],
  "properties": {
    "primitive_contract": {
      "type": "string"
    },
    "swapper_impl": {
      "$ref": "#/definitions/SwapperImpl"
    }
  },
  "definitions": {
    "AndrAddress": {
      "type": "object",
      "required": [
        "identifier"
      ],
      "properties": {
        "identifier": {
          "description": "Can be either an address or identifier of an ADO in a mission.",
          "type": "string"
        }
      }
    },
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "InstantiateInfo": {
      "type": "object",
      "required": [
        "ado_type",
        "msg"
      ],
      "properties": {
        "ado_type": {
          "description": "The ADO type. Used to retrieve the code id.",
          "type": "string"
        },
        "msg": {
          "description": "The instantiate message encoded in base64.",
          "allOf": [
            {
              "$ref": "#/definitions/Binary"
            }
          ]
        }
      }
    },
    "SwapperImpl": {
      "oneOf": [
        {
          "description": "Specifies the instantiation specification for the swapper impl.",
          "type": "object",
          "required": [
            "new"
          ],
          "properties": {
            "new": {
              "$ref": "#/definitions/InstantiateInfo"
            }
          },
          "additionalProperties": false
        },
        {
          "description": "Specifies the swapper impl by reference to an existing contract.",
          "type": "object",
          "required": [
            "reference"
          ],
          "properties": {
            "reference": {
              "$ref": "#/definitions/AndrAddress"
            }
          },
          "additionalProperties": false
        }
      ]
    }
  }
}
