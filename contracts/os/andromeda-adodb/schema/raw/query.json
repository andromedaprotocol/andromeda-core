          "type": "object",
          "required": [
            "code_id"
          ],
          "properties": {
            "code_id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "all_ado_type"
