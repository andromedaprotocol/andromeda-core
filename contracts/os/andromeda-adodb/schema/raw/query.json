        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "ado_metadata"
      ],
      "properties": {
        "ado_metadata": {
          "type": "object",
          "required": [
            "ado_type"
          ],
          "properties": {
            "ado_type": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "action_fee"
